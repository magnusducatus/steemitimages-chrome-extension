{
	"manifest_version": 2,
	"name": "GolosImages",
	"version": "1.0",
	"icons": { 
		"16": "icon_16.png",
		"19": "icon_19.png",
		"32": "icon_32.png",
		"38": "icon_38.png",
		"48": "icon_48.png",
		"128": "icon_128.png"
	},
	"background": {
		"persistent": false,
		"scripts": [ "bg.js" ]
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"browser_action": {
		"default_icon": "icon_128.png",
		"default_title": "GolosImages"
	}
}